[colors]
black                               = #000000
subtle                              = #1e1e1e
blue                                = #03597e
white                               = #ffffff
purple                              = #6d6dae                           
red                                 = #df4949
green                               = #5f875f
teal                                = #53a4cc
[bar/mainbar-center]

width                               = 4.55%
height                              = 16
offset-x                            = 47.5%
offset-y                            = 5
radius                              = 3
fixed-center                        = true

border-size                         = 4
border-color                        = #1e1e1e

font-0                              = FantasqueSansMono:style=Bold:pixelsize=14;3
font-1                              = Font Awesome:style=Solid:pixelsize=14;3
font-2                              = FantasqueSansMono Nerd Font:pixelsize=10;2

modules-center                      = logo logo1 logo2 logo3

