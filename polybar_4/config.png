width                               = 4.65%
height                              = 16
offset-x                            = 73.9%
offset-y                            = 5
radius                              = 3
fixed-center                        = true

border-size                         = 4
border-color                        = #1e1e1e

padding-left                        = 2
padding-right                       = 2

module-margin-left                  = 1
module-margin-right                 = 2

